{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:8080",
  "dependencies": {
    "@forte-music/mock": "^0.0.42",
    "@forte-music/schema": "^0.0.42",
    "apollo": "^1.3.0",
    "apollo-boost": "^0.1.22",
    "graphql": "^0.13.0",
    "graphql-tag": "^2.9.2",
    "intersection-observer": "^0.5.0",
    "lodash": "^4.17.10",
    "polished": "^1.9.3",
    "react": "^16.2.0",
    "react-apollo": "^2.1.8",
    "react-dom": "^16.2.0",
    "react-intersection-observer": "^3.0.3",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-side-effect": "^1.1.3",
    "react-virtual-list": "^2.3.0",
    "redux": "^3.0.0",
    "redux-devtools-extension": "^2.13.2",
    "styled-components": "^3.3.3"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.10",
    "@types/graphql": "^0.13.0",
    "@types/jest": "^23.1.0",
    "@types/lodash": "^4.14.116",
    "@types/react-dom": "^16.0.6",
    "@types/react-redux": "^6.0.2",
    "@types/react-router-dom": "^4.2.7",
    "@types/react-side-effect": "^1.1.0",
    "apollo-server-express": "^2.3.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jest-enzyme": "^6.0.1",
    "prettier": "1.9.2",
    "react-cosmos": "^4.6.4",
    "react-cosmos-telescope": "^4.6.4",
    "react-display-name": "^0.2.4",
    "react-scripts": "^2.1.1",
    "surge": "^0.20.1",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.2.2"
  },
  "resolutions": {
    "graphql": "0.13.0"
  },
  "config": {
    "prettierFiles": "'*.js' src/**/*.{js,css,ts,tsx} README.md"
  },
  "scripts": {
    "fmt": "prettier --write $npm_package_config_prettierFiles",
    "check-all": "yarn query-codegen && yarn lint && CI=true yarn test",
    "cosmos": "cosmos",
    "cosmos:export": "cosmos-export",
    "start-mock": "REACT_APP_MOCK_RESOLVER=true yarn start",
    "start": "react-scripts start",
    "build": "yarn query-codegen && react-scripts build",
    "test": "react-scripts test",
    "lint": "yarn tsc && yarn check-fmt && yarn tslint",
    "tslint": "tslint --project .",
    "check-fmt": "prettier --list-different $npm_package_config_prettierFiles",
    "query-codegen": "apollo codegen:generate --schema node_modules/@forte-music/schema/schema.json --queries 'src/**/*.{graphql,ts,tsx}' --target typescript"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
